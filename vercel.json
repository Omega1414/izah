{
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ],
        "builds": [
          {
            "src": "server.js",
            "use": "@vercel/node"
          }
        ],
        "routes": [
          {
            "src": "/(.*)",
            "dest": "/server.js"
          } 
        ],
      
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=300, must-revalidate"
          }
        ]
      }
    ]
  }
  